def shellsort(list1):
    
    gap = len(list1) // 2
    while gap > 0:

        for i in range(gap, len(list1)):
            temp = list1[i]
            j = i
# Sort the sub list for this gap

            while j >= gap and list1[j - gap] > temp:
                list1[j] = list1[j - gap]
                j = j-gap
            list1[j] = temp

# Reduce the gap for the next element

        gap = gap//2

list2 = [19,2,31,45,30,11,121,27]

shellsort(list2)
print(list2)
